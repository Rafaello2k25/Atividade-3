programa
{
	
	funcao inicio()
	{
		real numero1, numero2

		escreva("Informe um número. \n")
			leia(numero1)
		escreva("Informe outro numero. \n")
			leia(numero2)

		se(numero1 > numero2){
			escreva("O número ", numero1, " é maior.")
		}
		senao{
			escreva("O número ", numero2, " é maior.")
		}



		
	}
}
